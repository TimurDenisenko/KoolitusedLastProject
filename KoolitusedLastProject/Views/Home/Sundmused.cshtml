@model IEnumerable<KoolitusedLastProject.Models.Sundmus>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Sündmused</title>
</head>
<body>
    <h2>Sündmused</h2>

    <p>
        @Html.ActionLink("Loo uus", "su_Create")
    </p>
    <table class="table">
        <tr>
            <th>
                Nimetus
            </th>
            <th>
                Kirjeldus
            </th>
            <th>
                Õpetaja ID
            </th>
            <th>
                Ürituse päev
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Sundmusenimetus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Sundmusekirjeldus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OpetajaId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Uritusepaev)
                </td>
                <td>
                    @Html.ActionLink("Muuda", "su_Edit", new { id = item.Id }) |
                    @Html.ActionLink("Kustuta", "su_Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</body>
</html>
